{"protocolSection": {"identificationModule": {"nctId": "NCT00404547", "orgStudyIdInfo": {"id": "BY9010/CA-102"}, "organization": {"fullName": "AstraZeneca", "class": "INDUSTRY"}, "briefTitle": "Asthma Care With Alvesco\u00ae (Ciclesonide) in Primary Care in Adults - The ACCEPT-study (BY9010/CA-102)", "officialTitle": "An Open-Label Randomized, Multicenter Study in Patients With Asthma to Evaluate the Effectiveness of Alvesco\u00ae (Ciclesonide) Compared to Asthma Usual Care in a Primary Practice Setting"}, "statusModule": {"statusVerifiedDate": "2016-09", "overallStatus": "COMPLETED", "expandedAccessInfo": {"hasExpandedAccess": false}, "startDateStruct": {"date": "2006-11"}, "primaryCompletionDateStruct": {"date": "2008-04", "type": "ACTUAL"}, "completionDateStruct": {"date": "2008-12", "type": "ACTUAL"}, "studyFirstSubmitDate": "2006-11-27", "studyFirstSubmitQcDate": "2006-11-27", "studyFirstPostDateStruct": {"date": "2006-11-28", "type": "ESTIMATED"}, "resultsFirstSubmitDate": "2010-03-08", "resultsFirstSubmitQcDate": "2010-03-08", "resultsFirstPostDateStruct": {"date": "2010-03-26", "type": "ESTIMATED"}, "dispFirstSubmitDate": "2009-11-19", "dispFirstSubmitQcDate": "2010-03-08", "dispFirstPostDateStruct": {"date": "2010-03-26", "type": "ESTIMATED"}, "lastUpdateSubmitDate": "2016-12-02", "lastUpdatePostDateStruct": {"date": "2017-02-01", "type": "ESTIMATED"}}, "sponsorCollaboratorsModule": {"responsibleParty": {"type": "SPONSOR"}, "leadSponsor": {"name": "AstraZeneca", "class": "INDUSTRY"}}, "oversightModule": {"oversightHasDmc": false}, "descriptionModule": {"briefSummary": "The purpose of this study is to investigate the effectiveness of Alvesco\u00ae (Ciclesonide) compared with usual asthma care in the primary care setting. Patients with a history of asthma for at least 6 months and who, in the opinion of the physician, meet the clinical requirements for treatment with inhaled steroids (ICS) will be enrolled. They will either receive Alvesco\u00ae or usual care."}, "conditionsModule": {"conditions": ["Asthma"], "keywords": ["Asthma", "Alvesco\u00ae", "Ciclesonide"]}, "designModule": {"studyType": "INTERVENTIONAL", "phases": ["PHASE4"], "designInfo": {"allocation": "RANDOMIZED", "interventionModel": "PARALLEL", "primaryPurpose": "TREATMENT", "maskingInfo": {"masking": "NONE"}}, "enrollmentInfo": {"count": 1121, "type": "ACTUAL"}}, "armsInterventionsModule": {"armGroups": [{"label": "Alvesco", "type": "ACTIVE_COMPARATOR", "description": "Alvesco 320mcg / Alvesco 640mcg", "interventionNames": ["Drug: Ciclesonide"]}, {"label": "Usual Care", "type": "ACTIVE_COMPARATOR", "interventionNames": ["Drug: Usual Care Inhaled Glucocorticosteroids"]}], "interventions": [{"type": "DRUG", "name": "Ciclesonide", "description": "Asthma Care with Alvesco", "armGroupLabels": ["Alvesco"]}, {"type": "DRUG", "name": "Usual Care Inhaled Glucocorticosteroids", "description": "Usual asthma care and dosage as chosen by the Primary Care Physician including inhaled steroid treatment", "armGroupLabels": ["Usual Care"]}]}, "outcomesModule": {"primaryOutcomes": [{"measure": "Change in Mean of Total Score of Asthma Control Questionnaire (ACQ)", "description": "The score of the change from baseline ranges from -6 (=best possible outcome) to 6 (=worst possible outcome).", "timeFrame": "At the middle and end of the 12 week treatment period"}], "secondaryOutcomes": [{"measure": "Change in Patient Assessment of Asthma Control", "description": "Patient assessment of asthma control was assessed at baseline and at week 12 using the question \"How would you rate the control of your asthma symptoms?\". The change in this assessment was categorized in \"Improvement\" and \"Non-Improvement\".", "timeFrame": "At baseline and at week 12"}, {"measure": "Assessment of Patient Compliance During the Study", "timeFrame": "12 weeks"}, {"measure": "Assessment of Patient Treatment Satisfaction", "timeFrame": "12 weeks"}]}, "eligibilityModule": {"eligibilityCriteria": "Main inc